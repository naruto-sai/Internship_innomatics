   "source": [
    "class Solution:\n",
    "    def xorOperation(self, n: int, start: int) -> int:\n",
    "        nums=[start + 2 * i for i in range(n)]\n",
    "        xor=nums[0]\n",
    "        for i in range(1,n):\n",
    "            xor=xor^nums[i]\n",
    "        return xor"
   ]
  }
