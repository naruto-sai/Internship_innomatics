   "source": [
    "class Solution:\n",
    "    def subtractProductAndSum(self, n: int) -> int:\n",
    "        prod=1\n",
    "        sum_=0\n",
    "        while n>0:\n",
    "            prod*=n%10\n",
    "            sum_+=n%10\n",
    "            n=n//10\n",
    "        return prod-sum_"
   ]
  }
